{
    "source": {
        "include": [
        "./app",
        "./config"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "Magic Conch Shell Bot"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/dev/manual",
        "readme": "README.md",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/minami",
        "tutorials": "docs/dev/tutorials"
    }
}