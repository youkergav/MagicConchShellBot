{
    "source": {
        "include": [
        "./app",
        "./config"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}