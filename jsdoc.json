{
    "source": {
        "include": [
        "./app",
        "./config"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "MagicConchShellBot"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/jsdoc-template"
    }
}